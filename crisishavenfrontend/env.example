# CrisisHaven Environment Variables
# Copy this file to .env and fill in your actual values

# OmniDimension API Configuration
REACT_APP_OMNIDIM_API_KEY=your_actual_omnidim_api_key
REACT_APP_OMNIDIM_AGENT_ID=your_actual_agent_id

# API Mode (set to 'true' to use mock API for testing)
REACT_APP_MOCK_API=false

# Backend API URL (for future backend integration)
REACT_APP_BACKEND_URL=http://localhost:5000

# MongoDB Connection (for future backend)
MONGODB_URI=mongodb://localhost:27017/crisishaven

# JWT Secret (for future authentication)
JWT_SECRET=your_super_secret_jwt_key_here

# Port for the backend server
PORT=5000 